<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id ="country" class="com.inject.spring.beans.Country">
		<property name="name" value="Colombia"></property>
		<property name="years" value="1000"></property>
		<property name="city" ref="city"></property>
	</bean>
	
	<bean id ="city" class="com.inject.spring.beans.City">
		<property name="name" value="Bogota" ></property>
		<property name="years" value="100"></property>
	</bean>
	
</beans>
